    <!-- START: UTM Capture Script -->
    <script>
      document.addEventListener('DOMContentLoaded', function () {
        var queryParams = new URLSearchParams(window.location.search);

        // Function to add a hidden field to the form
        function addHiddenField(name, value) {
          var input = document.createElement("input");
          input.type = "hidden";
          input.name = name;
          input.value = value;
          // Change the selector below to target your form
          document.querySelector("form").appendChild(input);
        }

        // Define the UTM parameters we want to capture
        var utmParams = ["utm_source", "utm_medium", "utm_campaign", "utm_term", "utm_content"];

        // Check if UTM parameters are present and add them as hidden fields to the form
        utmParams.forEach(function (param) {
          var value = queryParams.get(param);
          if (value !== null) {
            addHiddenField(param, value);
          }
        });
      });
    </script>
    <!-- End UTM Capture Script -->

